<div id="header">
	<a href="<%= home_path %>" class="logo">
		<img src="<%= image_path('logo.jpg')%>">
	</a>
</div>
<div class="all">
	<div class="nav">
		<ul>
			<li><%= link_to "Home", home_path %></li>
			<li><%= link_to "About", about_path %></li>
			<% if account_signed_in? %>
                <% if current_account.status==1 %>
                    <li><%= link_to "Manage", dashboard_path %></li>
                <% else %>
                    <li><%= link_to "Profil", user_path(current_account) %></li>
                <% end %>
            <% else %>
                <li><%= link_to "Login", login_path %></li>
            <% end %>
		</ul>
	</div>
<div id="body">
<h1><span>Dashboard</span></h1>
</div>
	<div class="button" style="height:180px;text-align: center;">
		<%= link_to "Kelola Member", users_path %>
		<%= link_to "Lihat Profil", user_path(current_account) %>
		<%= link_to "Kembali", home_path %>
		<%= link_to "Logout", logout_path %>
	</div>
</div>
<div id="footer">
    <div>
        <p>&copy; 2017 Achtzehn</p>
    </div>
</div>